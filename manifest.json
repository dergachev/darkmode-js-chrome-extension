{
  "manifest_version": 2,

  "name": "DarkMode Alex",
  "description": "This extension will run darkmodejs",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": [
   "activeTab"
   ],
   "background": {
    "scripts": ["background.js"],
    "persistent": false
  }

}